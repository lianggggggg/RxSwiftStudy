platform :ios, '9.0'

use_frameworks!
target 'RxSwiftstudy' do
pod 'RxSwift'
pod 'RxCocoa'
pod 'NSObject+Rx'  #为我们提供 rx_disposeBag 
pod 'Moya/RxSwift'
pod 'ObjectMapper'
pod 'RxDataSources'  #帮助我们优雅的使用tableView的数据源方法
pod 'Then'  # 提供快速初始化的语法糖
pod 'Kingfisher'
pod 'Reusable' #帮助我们优雅的使用自定义cell和view,不再出现Optional
pod 'MJRefresh'
pod 'SVProgressHUD'


end
